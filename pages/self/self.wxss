.UCenter-bg {
  background: #3688ff;
  background-size: cover;
  height: 550rpx;
  display: flex;
  justify-content: center;
  padding-top: 40rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg image {
  width: 200rpx;
  height: 200rpx;
}

.UCenter-bg .gif-wave {
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;
  height: 100rpx;
}

map, .mapBox {
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;
  height: 100rpx;
}

map, .mapBox {
  width: 750rpx;
  height: 300rpx;
}

/*
* 伪圆角效果
*/

.circleIcon {
  overflow: hidden;
  display: block;
  width: 160rpx;
  height: 160rpx;
  margin: 20rpx;
  margin-top: 50rpx;
  border-radius: 50%;
  border: 2px solid #fff;
  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);
}

.tab-group {
  z-index: 99;
  width: 100%;
  height: 110rpx;
  background: #f1f1f1;
}

.nav .cu-item {
  height: 80rpx;
  line-height: 80rpx;
}

.cu-item {
  padding: 20rpx;
}

.fold-item.active .cu-item .content {
  opacity: 1;
}

.fold-content-inner {
  padding: 0 20rpx !important;
}

.cu-item-title {
  min-height: 80rpx;
  padding: 20rpx 0 0 0 !important;
  font-size: 30rpx;
}

.rate-view {
  margin-top: 6rpx;
}

.rate-table .year {
  display: inline-block;
  width: 20%;
  text-align: center;
}

.rate-table .rate-show {
  display: inline-block;
  width: 55%;
}

.rate-table .rate-data {
  display: inline-block;
  width: 25%;
  text-align: center;
}

.mark_rank-table {
  margin-top: 20rpx;
  text-align: center;
}

.mark_rank-table-tr {
  border-top: 2rpx solid #eee;
}

.mark_rank-table-tr .year {
  display: inline-block;
  width: 20%;
}

.mark_rank-table-tr .mark_rank {
  display: inline-block;
  box-sizing: border-box;
  width: 40%;
  border-left: 2rpx solid #eee;
}

.animation-show {
  animation-name: animation-show;
  animation-duration: 0.5s;
}

.animation-hide {
  animation-name: animation-hide;
  animation-duration: 0.5s;
}

@-webkit-keyframes animation-show {
  0% {
    opacity: 0;
    transform: scale(0);
  }

  100% {
    opacity: 1;
    transform: scale(1);
  }
}

@keyframes animation-show {
  0% {
    opacity: 0;
    transform: scale(0);
  }

  100% {
    opacity: 1;
    transform: scale(1);
  }
}

@-webkit-keyframes animation-hide {
  0% {
    opacity: 1;
    transform: scale(1);
  }

  100% {
    opacity: 0;
    transform: scale(0);
  }
}

@keyframes animation-hide {
  0% {
    opacity: 1;
    transform: scale(1);
  }

  100% {
    opacity: 0;
    transform: scale(0);
  }
}

.comment {
  overflow: auto;
  margin-top: 20rpx;
  padding-bottom: 16rpx;
  border-bottom: 2rpx solid #eee;
}

.comment:first-child {
  margin-top: 0;
}

.comment:last-child {
  border-bottom: 0;
  padding-bottom: 0;
}

.comment-user-info {
  padding-left: 120rpx;
}

.comment-content {
  margin-top: 16rpx;
}

.comment-content-fold text {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 6;
}

.show-foot-btn {
  position: fixed;
  right: 20rpx;
  bottom: 0;
  width: 70rpx;
  height: 30rpx;
  margin-bottom: 8rpx;
  padding: 0;
  line-height: 1;
}

.setting-label {
  display: block;
  margin-bottom: 20rpx;
}

.setting-label:last-child {
  margin-bottom: 0;
}

.radio-text {
  margin-left: 10rpx;
}

.setting-submit-btn {
  padding: 0;
  border-radius: 0;
}

.setting-submit-btn::after {
  border: none;
}

.cur {
  border-bottom: 4rpx solid;
  margin-top: 0 10rpx;
}

.goods-container {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  box-sizing: content-box;
  padding: 24rpx;
  background-color: #f2f2f2;
  font-size: 30rpx;
}

.goods-box {
  width: 339rpx;
  height: 472rpx;
  background-color: #fff;
  overflow: hidden;
  margin-bottom: 24rpx;
  border-radius: 5px;
}

.goods-box .img-box {
  width: 339rpx;
  height: 339rpx;
  overflow: hidden;
}

.goods-box .img-box image {
  width: 339rpx;
  height: 339rpx;
}

.goods-box .goods-title {
  width: 280rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  padding: 24rpx 0 0rpx 0;
  color: #000;
  margin-left: 24rpx;
}

.goods-box .goods-price {
  width: 280rpx;
  overflow: hidden;
  font-size: 28rpx;
  padding: 10rpx 0;
  color: #e64340;
  margin-left: 24rpx;
}

.no-more-goods {
  text-align: center;
  font-size: 24rpx;
  padding-bottom: 48rpx;
  color: #999;
}

.sell {
  display: flex;
  justify-content: center;
  position: relative;
  top: -200rpx;
  font-size:70rpx;
  color: #ef4340;
  border:5rpx solid #e64340 ;
  padding:10%;
  margin: 0 13rpx
}


.center {
  display: flex;
  justify-content: center;
  align-items: center;
}